{
    "name": "dcplibrary/shoutbomb-failure-reports",
    "description": "Laravel package to ingest and parse Shoutbomb failure reports from Microsoft 365 Outlook using Graph API",
    "type": "library",
    "keywords": ["laravel", "shoutbomb", "microsoft-graph", "outlook", "sms", "voice", "failure-reports"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Lashbrook",
            "email": "blashbrook@dcplibrary.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.5",
        "microsoft/microsoft-graph": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dcplibrary\\ShoutbombFailureReports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcplibrary\\ShoutbombFailureReports\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcplibrary\\ShoutbombFailureReports\\Providers\\ShoutbombFailureReportsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
